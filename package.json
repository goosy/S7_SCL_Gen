{
  "name": "s7-scl-gen",
  "version": "1.0.0",
  "type": "module",
  "description": "S7 PLC SCL 代码自动生成",
  "main": "/lib/index.js",
  "exports": {
    ".": [
      {
        "import": "./lib/index.js",
        "require": "./lib/index.cjs",
        "default": "./lib/acorn.js"
      },
      "./lib/index.js"
    ]
  },
  "engines": {
    "node": ">=16.7.0"
  },
  "dependencies": {
    "gooconverter": "^0.3.6",
    "iconv-lite": "^0.6.3",
    "mri": "^1.2.0",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "eslint": "^8.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.3"
  },
  "directories": {
    "conf": "conf",
    "lib": "lib"
  },
  "bin": {
    "s7scl": "cli.js"
  },
  "scripts": {
    "clean": "rimraf ./example/SCL ./lib",
    "dev": "yarn clean && node ./dev.js",
    "build": "node ./build.js",
    "prepublish": "yarn clean && yarn build",
    "watch": "rollup -c -w",
    "test": "mocha"
  },
  "keywords": [
    "modbusTCP",
    "SCL",
    "S7"
  ],
  "author": "goosy.jo@gmail.com",
  "license": "MIT"
}
