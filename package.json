{
  "name": "s7-scl-gen",
  "version": "0.5.5",
  "type": "module",
  "description": "S7 PLC SCL 代码自动生成",
  "main": "/src/index.js",
  "exports": {
    ".": [
      {
        "import": "./lib/index.js",
        "require": "./lib/index.cjs",
        "default": "./lib/acorn.js"
      },
      "./lib/index.js"
    ]
  },
  "engines": {
    "node": ">=14.8.0"
  },
  "dependencies": {
    "gooconverter": "^0.3.5",
    "iconv-lite": "^0.6.3",
    "js-yaml": "^4.1.0",
    "mri": "^1.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "eslint": "^8.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.3"
  },
  "directories": {
    "conf": "conf",
    "lib": "lib"
  },
  "bin": {
    "s7scl": "cli.js"
  },
  "scripts": {
    "clean": "rimraf ./conf/*.zyml ./conf/AS1/**/",
    "dev": "node ./dev.js",
    "build": "yarn clean && rollup -c",
    "prepublish": "yarn build",
    "watch": "rollup -c -w"
  },
  "keywords": [
    "modbusTCP",
    "SCL",
    "S7"
  ],
  "author": "goosy.jo@gmail.com",
  "license": "MIT"
}
