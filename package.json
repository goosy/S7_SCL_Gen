{
    "name": "s7-scl-gen",
    "version": "1.15.20",
    "type": "module",
    "description": "S7 PLC SCL 代码自动生成",
    "main": "/lib/index.js",
    "exports": {
        ".": [
            {
                "import": "./lib/index.js"
            },
            "./lib/index.js"
        ]
    },
    "engines": {
        "node": ">=16.7.0"
    },
    "dependencies": {
        "iconv-lite": "^0.6.3",
        "nodemon": "^3.1.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "eslint": "^8.57.0",
        "globby": "^13.2.2",
        "gooconverter": "^1.0.2",
        "mri": "^1.2.0",
        "rimraf": "^5.0.5",
        "rollup": "^4.12.0",
        "yaml": "^2.4.0"
    },
    "directories": {
        "conf": "conf",
        "lib": "lib"
    },
    "bin": {
        "s7scl": "./lib/cli.js"
    },
    "scripts": {
        "clean": "rimraf -g ./example/*/ ./lib",
        "dev": "rimraf -g ./example/*/ && node ./dev.js",
        "build": "node ./build.js",
        "prepublish": "pnpm clean && pnpm build",
        "watch": "nodemon ./build.js",
        "test": "mocha"
    },
    "keywords": [
        "modbusTCP",
        "SCL",
        "S7"
    ],
    "author": "goosy.jo@gmail.com",
    "license": "MIT"
}
